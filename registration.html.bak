        <div class="row">
            <div class="col-sm-2"></div>
            <div class="col-sm-4 text-center" >
                <h1 style="color: antiquewhite; font-family:cursive;">Registration</h1>   
                <form class="form-horizontal" action="#" method="post">
                    <div class="form-group">
                        <label class="control-label col-sm-3" for="txtfname" >fname:</label>
                        <div class="col-sm-8">
                        <input type="text" class="form-control" name="name" id="fname" placeholder="Enter your first name"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-3" for="txtlname" >Lname:</label>
                        <div class="col-sm-8">
                        <input type="text" class="form-control" name="name" id="lname" placeholder="Enter your last name"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-3" for="mono" id="mono">Cno:</label>
                        <div class="col-sm-8">
                        <input type="text" class="form-control" name="mono" id="mono" placeholder="Enter contect number"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-3" for="email" id="email" >Email:</label>
                        <div class="col-sm-8">
                        <input type="text" class="form-control" name="email" id="email" placeholder="Enter email id"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-3" for="city" id="city">City:</label>
                        <div class="col-sm-3">
                        <select name="city">
                            <option value="surat">surat</option>
                            <option value="Bharuch">Bharuch</option>
                            <option value="Vadodara">Vadodara</option>
                        </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-sm-11">
                            <input type="checkbox" />
                            <label class="form-check-label" for="remember me">Remember me</label>                                </div>
                    <div class="form-group">
                        <div class="col-sm-9">
                        <button type="submit" class="btn-success">Submit</button>
                    </div>
                    </div>
                    </div>
                    </div>
                </form> 
            </div>
        </div>
    
